@{
@ViewData["Title"]
}

<div class="">
  <div class="container">
    <br>
    <div class="row">
      <h5 class="dh right-align">ގެލަރީ</h5>
      <hr>

      @foreach (var item in ViewBag.Images)
      {

      <div class="col m3 s12">
        <img src="~/gallery/@item.ImgUrl" data-caption="@item.Text" style="width: 280px;height: 250px;" class="materialboxed responsive-img">

      </div>

      }

    </div>
    <br>
  </div>
</div>

<script>
  var instance = M.Carousel.init({
    fullWidth: true,
    indicators: true
  });

  // Or with jQuery

  $('.carousel.carousel-slider').carousel({
    fullWidth: true,
    indicators: true
  });
</script>