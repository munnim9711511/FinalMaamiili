<div class="">
  <div class="container ">
    <br />
    <h5 class="left-align">Cafey</h5>
    <hr />
    <div class="row">
      @foreach (var item in ViewBag.cafeyEn) {

      <div class="col s12 m6">
        <div class="card">
          <div class="card-image">
            <img
              src="~/LinkPic/@item.ImgUrl "
              alt=""
              class="responsive-img center-align"
              height="484px"
              width="750px"
            />
           
          </div>
          <div class="card-content">
              <h6>
                  @item.Title
              </h6>
              <hr>
            <p>
                @item.Text
            </p>
          </div>
        </div>
      </div>
     

      }
    </div>
  </div>
</div>
