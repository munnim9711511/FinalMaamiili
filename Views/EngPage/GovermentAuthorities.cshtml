<div class="">
<div class="container center-align">
    <br>
   <h5 class="left-align">
     Goverment Authorities
   </h5>
   <hr>
<div class="row">
    <div class="col s12  left-align">
        
        @foreach (var item in  ViewBag.govAuth )
        {
            <h5 class="dh " >@item.Title</h5>
        <hr>
            <img src="~/LinkPic/@item.ImgUrl" class="responsive-img" height="484px" width="750px">
             <p  style="text-align: justify; overflow-wrap: break-word;">
            @item.Text
        </p>
        }
       
    </div>
</div>
</div>
</div>